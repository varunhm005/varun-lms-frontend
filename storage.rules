rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // match /{allPaths=**} {
    //   allow read;
    //   allow write: if false;
    // }
    match /user_images/{imageId} {
      // Only allow uploads of any image file that's less than 5MB
      allow write: if request.auth != null;

      // Allow reads for all
      allow read;
    }
    match /question_scenario_mages/{imageId} {
      // Only allow uploads of any image file that's less than 5MB
      allow write: if request.auth != null;

      // Allow reads for all
      allow read;
    }
  }
}
