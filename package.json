{
  "name": "lms-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite --port 7051",
    "build": "GENERATE_SOURCEMAP=false tsc && vite build",
    "build:qa": "GENERATE_SOURCEMAP=false tsc && vite build --mode qa",
    "build:dev": "GENERATE_SOURCEMAP=false tsc && vite build --mode development",
    "lint": "eslint --cache src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "cz",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "concurrently \"npm run start:dev\" \"npm run codegen\"",
    "deploy:stage": "./scripts/deploy.sh -e staging",
    "deploy:prod": "./scripts/deploy.sh -e production"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@apollo/client": "^3.10.2",
    "@cyntler/react-doc-viewer": "^1.14.1",
    "@sentry/react": "^7.113.0",
    "@sentry/vite-plugin": "^2.16.1",
    "antd": "^5.6.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "firebase": "^9.23.0",
    "graphql": "^16.8.1",
    "js-file-downloader": "^1.1.25",
    "localforage": "^1.10.0",
    "lottie-react": "^2.4.0",
    "match-sorter": "^6.3.4",
    "nanoid": "^4.0.2",
    "react": "^18.3.1",
    "react-doc-viewer": "^0.1.14",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-json-to-excel": "^1.0.7",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.23.0",
    "react-text-truncate": "^0.19.0",
    "react-to-print": "^2.15.1",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^3.0.2",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.7",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-text-truncate": "^0.14.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.3.5",
    "husky": "^8.0.3",
    "postcss": "^8.4.38",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "sass": "^1.77.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^4.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
